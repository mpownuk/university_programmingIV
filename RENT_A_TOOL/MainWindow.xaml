<Window x:Class="RENT_A_TOOL.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RENT_A_TOOL"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Label Content="Witamy w Rent-a-Tool!" HorizontalAlignment="Center" VerticalAlignment="Top"/>
        <Button Content="Zaloguj się" HorizontalAlignment="Left" VerticalAlignment="Center" Width="100" Click="Login_Click" Margin="111,0,0,0"/>
        <Button Content="Zarejestruj się" HorizontalAlignment="Left" VerticalAlignment="Center" Width="100" Click="Register_Click" Margin="589,0,0,0"/>
    </Grid>
</Window>
